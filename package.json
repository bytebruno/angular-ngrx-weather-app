{
  "name": "weather-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port 4200",
    "build": "ng build --configuration production",
    "test": "ng test",
    "e2e": "ng e2e",
    "style:fix": "prettier --write \"**/{src,tests,e2e}/*.{scss,ts}\" && js-beautify \"src/**/*.html\"",
    "lint:fix": "tslint --config tslint.json --fix --project . -e \"**/{test,polyfills}.ts\"",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer ./dist/angular-ngrx-weather-app/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.12",
    "@angular/cli": "^14.2.12",
    "@angular/compiler-cli": "^14.3.0",
    "@ngrx/schematics": "^13.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "js-beautify": "^1.13.5",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.13.9",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.6.4",
    "webpack-bundle-analyzer": "^4.4.0"
  }
